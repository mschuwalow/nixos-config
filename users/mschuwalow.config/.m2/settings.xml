				<!-- END: Added for python deployments -->
				<repository>
					<id>atlassian-public</id>
					<url>https://maven.atlassian.com/repository/public</url>
					<snapshots>
						<enabled>true</enabled>
						<updatePolicy>never</updatePolicy>
						<checksumPolicy>warn</checksumPolicy>
					</snapshots>
					<releases>
						<enabled>true</enabled>
						<checksumPolicy>warn</checksumPolicy>
					</releases>
				</repository>
                <repository>
                    <id>aim-fs-repo-releases</id>
                    <name>fileserver release repository</name>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <url>file:///mnt/aim-aim/aim-fs-maven-repo/releases</url>
				</repository>
                <repository>
                    <id>aim-fs-repo-snapshots</id>
                    <name>fileserver snapshot repository</name>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <url>file:///mnt/aim-aim/aim-fs-maven-repo/snapshots</url>
				</repository>
			</repositories>

			<pluginRepositories>
