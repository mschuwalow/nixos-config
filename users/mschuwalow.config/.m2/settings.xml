	<profiles>
		<profile>
			<id>aim-profile</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>aim-repo-releases</id>
					<name>aim release repository</name>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<url>https://nexus.agile-it-management.de/repository/releases/</url>
				</repository>
				<repository>
					<id>aim-repo-snapshots</id>
					<name>aim snapshot repository</name>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<url>https://nexus.agile-it-management.de/repository/snapshots/</url>
				</repository>
				<!-- START: Added for python deployments -->
				<repository>
					<id>aim-repo-python</id>
					<name>aim snapshot repository</name>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<url>https://nexus.agile-it-management.de/repository/aim-pypi/</url>
				</repository>
				<repository>
					<id>aim-pypi</id>
					<name>aim snapshot repository</name>
					<releases>
						<enabled>false</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<url>https://nexus.agile-it-management.de/repository/PyPI/</url>
					<!-- <url>https://pypi.org/</url> -->
